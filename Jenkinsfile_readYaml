pipeline {
    agent any
    stages {
        stage('clone') {
            steps {
                git credentialsId: 'git_creds', url: 'https://github.com/hrudayhh/jjcfw.git'
            }
        }
        stage('Read YAML file') {
            steps {
                script { 
                    def data = readYaml file: 'config.yml'
                    println(data.src)
                }
            }
        }
    }
}
