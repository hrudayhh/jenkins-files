pipeline {
    agent any
    stages {
        stage('clone') {
            steps {
                git credentialsId: 'git_creds', url: 'https://github.com/hrudayhh/jjcfw.git'
            }
        }
        stage('Read YAML file') {
            steps {
                script { 
                    def data = readYaml file: 'roles/requirements.yml'
                    def urls = data.src
                    for (repos in urls) {
                        sh "echo $repos"
                        sh "git ls-remote $repos"
                    }
                }
            }
        }
    }
}
